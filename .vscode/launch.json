{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 4200,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}/apps/client"
    },
    {
      "name": "Launch my-nest-project",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "server"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/apps/server"
    }
  ]
}
